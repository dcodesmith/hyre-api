{
  "name": "hyre-api-nestjs",
  "version": "1.0.0",
  "type": "commonjs",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "build": "nest build",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "biome lint --write .",
    "lint:check": "biome lint .",
    "check": "biome check --write .",
    "check:ci": "biome check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:cucumber": "cucumber-js --config cucumber.js",
    "test:cucumber:watch": "cucumber-js --watch",
    "test:cucumber:booking": "cucumber-js --profile booking",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "deploy": "pnpm run build && pnpm run db:generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@nestjs/bull": "^10.2.1",
    "@nestjs/common": "^10.4.8",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.8",
    "@nestjs/cqrs": "^10.0.7",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^10.4.8",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/terminus": "^10.2.3",
    "@prisma/client": "^6.12.0",
    "@react-email/components": "^0.3.2",
    "@react-email/render": "^1.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.11.0",
    "bull": "^4.16.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.1",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.4.0",
    "pino-pretty": "^13.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.2.2",
    "resend": "^2.1.0",
    "rxjs": "^7.8.1",
    "twilio": "^4.23.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@cucumber/cucumber": "10.8.0",
    "@cucumber/pretty-formatter": "^2.0.1",
    "@faker-js/faker": "^8.4.1",
    "@flydotio/dockerfile": "^0.7.10",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.8",
    "@types/bull": "^4.10.0",
    "@types/chai": "4.3.11",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/supertest": "^6.0.2",
    "callsites": "3.1.0",
    "chai": "4.3.10",
    "cucumber-tsflow": "^4.5.2",
    "jest": "^30.0.5",
    "prisma": "^6.13.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "overrides": {
      "callsites": "3.1.0"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}