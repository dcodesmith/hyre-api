{
  "name": "hyre-api-nestjs",
  "version": "1.0.0",
  "type": "commonjs",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "build": "prisma generate && nest build",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "biome lint --write .",
    "lint:check": "biome lint .",
    "check": "biome check --write .",
    "check:ci": "biome check .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:coverage:watch": "vitest --coverage --watch",
    "test:debug": "vitest --inspect-brk",
    "test:cucumber": "cucumber-js --config cucumber.cjs",
    "test:cucumber:watch": "cucumber-js --watch",
    "test:cucumber:booking": "cucumber-js --profile booking",
    "test:unit": "vitest --run",
    "test:unit:watch": "vitest --watch",
    "test:all": "pnpm test:unit && pnpm test:cucumber",
    "test:all:watch": "pnpm test:unit:watch & pnpm test:cucumber:watch",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "deploy": "pnpm run build && pnpm run db:generate",
    "test:coverage:ci": "vitest --coverage --run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@nestjs/bull": "^11.0.3",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/terminus": "^11.0.0",
    "@prisma/client": "^6.14.0",
    "@react-email/components": "^0.3.3",
    "@react-email/render": "^1.2.1",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.11.0",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.1",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.2.2",
    "resend": "^2.1.0",
    "rxjs": "^7.8.2",
    "twilio": "^4.23.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@cucumber/cucumber": "10.8.0",
    "@cucumber/pretty-formatter": "^2.1.0",
    "@faker-js/faker": "^8.4.1",
    "@flydotio/dockerfile": "^0.7.10",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/bull": "^4.10.4",
    "@types/chai": "4.3.11",
    "@types/express": "^4.17.23",
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "callsites": "3.1.0",
    "chai": "4.3.10",
    "cucumber-tsflow": "^4.5.2",
    "prisma": "^6.14.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "overrides": {
      "callsites": "3.1.0"
    }
  }
}
