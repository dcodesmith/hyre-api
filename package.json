{
  "name": "hyre-api-nestjs",
  "version": "1.0.0",
  "type": "commonjs",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "build": "prisma generate && nest build",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "biome lint --write .",
    "lint:check": "biome lint .",
    "check": "biome check --write .",
    "check:ci": "biome check .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:coverage:watch": "vitest --coverage --watch",
    "test:debug": "vitest --inspect-brk",
    "test:unit": "vitest --run",
    "test:e2e": "vitest --run test/e2e",
    "test:e2e:watch": "vitest --watch test/e2e",
    "test:all": "vitest --run",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "deploy": "pnpm run build && pnpm run db:generate",
    "test:coverage:ci": "vitest --coverage --run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.927.0",
    "@aws-sdk/s3-request-presigner": "^3.927.0",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/terminus": "^11.0.0",
    "@prisma/client": "^6.19.0",
    "@react-email/components": "^0.3.3",
    "@react-email/render": "^1.4.0",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.13.2",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.3",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.14.0",
    "pino-pretty": "^13.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.2.2",
    "resend": "^2.1.0",
    "rxjs": "^7.8.2",
    "twilio": "^4.23.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@faker-js/faker": "^8.4.1",
    "@flydotio/dockerfile": "^0.7.10",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@swc/core": "^1.15.1",
    "@types/bull": "^4.10.4",
    "@types/express": "^4.17.25",
    "@types/node": "^20.19.24",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "fishery": "^2.3.1",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.8",
    "vite-tsconfig-paths": "^5.1.4",
